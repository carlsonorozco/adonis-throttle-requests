{
  "name": "adonis-throttle-requests",
  "version": "1.0.0",
  "description": "Throttle requests for AdonisJs framework",
  "homepage": "https://github.com/carlsonorozco/adonis-throttle-requests",
  "bugs": {
    "url": "https://github.com/carlsonorozco/adonis-throttle-requests/issues"
  },
  "license": "MIT",
  "author": "Carlson Orozco <carlsonorozco@gmail.com>",
  "scripts": {
    "lint": "standard",
    "pretest": "npm run test:cleanup && npm run lint",
    "test:cleanup": "rm -rf test/functional/database && rm -rf test/unit/tmp",
    "posttest": "npm run test:cleanup && npm run coverage",
    "test:local": "npm run test:cleanup && FORCE_COLOR=true DB=sqlite node bin/index.js --local",
    "test": "snyk test && nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlsonorozco/adonis-throttle-requests.git"
  },
  "devDependencies": {
    "@adonisjs/fold": "^4.0.5",
    "adonis-cache": "^0.3.1",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^1.0.5",
    "japa-cli": "^1.0.1",
    "nyc": "^11.2.1",
    "snyk": "^1.62.1",
    "standard": "^10.0.3"
  },
  "keywords": [
    "adonis",
    "adonis-framework",
    "requests",
    "limit",
    "throttle",
    "rate",
    "limiter",
    "ratelimiter",
    "cache"
  ],
  "standard": {
    "globals": [
      "describe",
      "it"
    ]
  }
}
